<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="core.model.CoreExample" />
	<class name="core.model.impl.CoreExampleImpl" table="example_CoreExample">
		<id column="id_" name="id" type="long">
			<generator class="assigned" />
		</id>
		<property name="coreName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="coreType" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
</hibernate-mapping>